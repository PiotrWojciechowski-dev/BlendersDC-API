@model IEnumerable<BlendersDC_API.Models.Oil>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.OilTank1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OilTank2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OilTankInter)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedAt)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OilTank1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OilTank2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OilTankInter)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedAt)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
